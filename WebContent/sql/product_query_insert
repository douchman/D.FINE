drop table DfineProduct



CREATE TABLE DfineProduct
(
    productID        VARCHAR2(20)     NOT NULL, 
    brandName        VARCHAR2(30)     NOT NULL, 
    productName      VARCHAR2(50)     NOT NULL, 
    productDetail    CLOB             NOT NULL, 
    productPrice     INT              NOT NULL, 
    productStock     INT              NOT NULL, 
    productColor     VARCHAR2(20)     NOT NULL, 
    productSize      VARCHAR2(20)     NOT NULL, 
    snapshot         VARCHAR2(500)    NOT NULL, 
    CONSTRAINT DFINEPRODUCT_PK PRIMARY KEY (productID)
)

